{
  "include": [
    "__test__"
  ],
  "exclude": [
    ".cargo",
    ".github",
    "node_modules",
    "npm",
    "src"
  ],
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
  }
}